  "dependencies": {
    "as2d": "github:as2d/as2d",
    "assemblyscript": "github:assemblyscript/assemblyscript"
  },
  "devDependencies": {
    "parcel-bundler": "^1.11.0",
    "typescript": "^3.3.1"
  }
}
